
  <div class="container message-alert">
    <%- if notice %>
      <p class="alert-info alert">
        <%= notice %>
        <button type="button" class="close" data-dismiss="alert">×</button>
      </p>
    <% end %>
    <%- if alert %>
      <p class="alert-danger alert">
        <%= alert %>  
        <button type="button" class="close" data-dismiss="alert">×</button>
      </p>
    <% end %>
  </div>
