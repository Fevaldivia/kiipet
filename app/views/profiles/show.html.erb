 <div class="breadcrumb-wrap">
 </div><!--breadcrumbs-->
<br>
    <div class="container">
      <div class="panel panel-default">
      <div class="panel-heading">
        <p>
          hola
        </p>
      </div>
      <div class="panel-body">
        <div class="results-box">
          <div class="col-md-2">
            <%= image_tag @profile.avatar.url, class: 'img-circle', :size => "150x150"%>
          </div>
          <div class="col-md-10">
            <h4 class="profile_name"><%= @profile.name %></h4>
            <ul class="list-inline link-ul">
              <p><%= @profile.county.name%></p>
            </ul>
            <p>
              <p><%= @profile.bio %></p>
            </p>
            <hr>
          </div>

        </div><!--result box-->
      </div>
    </div>
        <div class="divide30"></div>
          <div class="divide30"></div>
            <div class="row">
              <div class="col-md-12">
                <div class="profile-service">
                   <% @profile.services.each do |ps| %>
                        <div class='service'>
                          <li class="col-md-3 services">
                            <div class="thumbnail">
                              <div class="caption">
                                <h4><%= ps.name %> </h4>
                                <p><%= ps.description%></p>
                              </div>
                              <span>
                              </span>
                              <span class="caption">
                                <p class="price-services">Precio:
                                  <%= ps.profile_services.get_price(@profile.id) %>
                                </p>
                              </span>
                            </div>
                          </li>
                        </div>
                      <% end %>
                  </div>
              </div>
              </div>
             <div class="divide30"></div>
        </div><!--full width container-->
        <hr>
      <div class="form-actions text-center">
        <%= link_to "Reservar", new_calendar_booking_path(@profile), class: "btn btn btn-info"%>
      </div>
      <br>
